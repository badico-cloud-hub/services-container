    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-react": "^7.9.0",
    "husky": "^0.15.0-rc.8",
    "jest": "^23.1.0",
    "jest-cli": "^23.2.0"
  },
  "jest": {
